{
  "name": "ynab",
  "version": "2.0.0-rc.0",
  "description": "YNAB API Javascript (Node) Library",
  "author": "YNAB",
  "email": "api@ynab.com",
  "url": "https://api.ynab.com",
  "keywords": [
    "typescript",
    "open-api",
    "ynab",
    "api"
  ],
  "files": [
    "dist/",
    "dist/esm",
    "dist/browser"
  ],
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/ynab/ynab-sdk-js.git"
  },
  "engines": {
    "node": "<=18"
  },
  "scripts": {
    "generate": "openapi-generator generate -i ./open_api_spec.yaml -g \"typescript-fetch\" --type-mappings Date=string --additional-properties=modelPropertyNaming=original,useSingleRequestParameter=false -t ./templates -o src",
    "download_lastest_spec": "wget https://api.ynab.com/papi/open_api_spec.yaml -O open_api_spec.yaml",
    "build": "tsc -b src/ test/ examples/ && tsc -b src/tsconfig.esm.json && webpack --mode production && npm run build_examples",
    "build_examples": "tsc -b examples/",
    "test": "TS_NODE_PROJECT=./test/tsconfig.json mocha --reporter spec --require ts-node/register/type-check 'test/**/*.ts'"
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/fetch-mock": "^7.3.2",
    "@types/mocha": "^8.0.2",
    "@types/node": "^14.0.27",
    "chai": "^4.2.0",
    "fetch-mock": "^9.10.6",
    "jsh": "0.34.0",
    "mocha": "^10.2.0",
    "ts-loader": "^8.0.2",
    "ts-node": "^8.10.2",
    "typescript": "4.5.5",
    "url": "^0.11.0",
    "webpack": "^5.74.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "fetch-ponyfill": "^7.1.0"
  }
}
